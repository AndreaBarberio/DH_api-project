{
    "name": "dh_api-project",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint src/ --cache --fix",
        "test": "jest",
        "build": "tsc",
        "build:dev": "tsc --watch",
        "start": "node --enable-source-maps dist/server.js",
        "start:dev": "nodemon --watch dist/",
        "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'"
    },
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndreaBarberio/DH_api-project.git"
    },
    "bugs": {
        "url": "https://github.com/AndreaBarberio/DH_api-project/issues"
    },
    "homepage": "https://github.com/AndreaBarberio/DH_api-project#readme",
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/jest": "^29.0.1",
        "@types/node": "^18.7.16",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "concurrently": "^7.4.0",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-n": "^15.2.5",
        "jest": "^29.0.2",
        "nodemon": "^2.0.19",
        "prettier": "2.7.1",
        "supertest": "^6.2.4",
        "ts-jest": "^29.0.0",
        "typescript": "^4.8.3"
    }
}
